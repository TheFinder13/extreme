<table border="3">
    <tr>
        <th>image </th>
        <th>nom </th>
        <th>desc </th>
        <th>prix </th>
        <th>reference </th>
        <th>Sous Category </th>
        <th>modif </th>
        <th>delete </th>
    </tr>
    {% for i in category %}
        <tr>

            <td> <img width="300" height="200" src=" {{ asset('uploads/' ~ i.image) }}"></td>

            <td>{{ i.nom }}</td>
            <td>{{ i.description }}</td>
            <td>{{ i.reference }}</td>
            <td>{{ i.prix }}</td>
            <td>{{ i.SubCategory }}</td>
            <td><a href="{{ path('produit',{'id':i.id}) }}">Modify</a></td>
            <td><a href="{{ path('suppProduit',{'id':i.id}) }}">Delete</a></td>

        </tr>
    {% endfor %}
</table>
